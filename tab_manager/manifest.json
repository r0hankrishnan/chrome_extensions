{
  "manifest_version": 3,
  "name": "Tab Session Manager with Notes",
  "version": "1.0",
  "description": "Save and restore tab sessions with notes",
  "permissions": ["tabs", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  }
}
